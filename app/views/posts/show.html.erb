<p id="notice"><%= notice %></p>

<p>
  <strong>User:</strong>
  <%= @post.user.username %>
</p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Body:</strong>
  <%= @post.body %>
</p>

<p>
  <strong>tl:dr:</strong>
  <% @post.summaries.each do |summary| %>
   <h3><%= summary.summary %></h3>
  <% end %>
</p>

<%= link_to 'Create tl:dr', new_user_post_summary_path(@post.user, @post) %> |

<%= link_to 'Edit', edit_user_post_path(@post) %> |
<%= link_to 'Back', user_posts_path %>
